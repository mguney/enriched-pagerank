<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<mapping wildcard="true">
		<class-a>edu.uci.ics.crawler4j.crawler.Page</class-a>
		<class-b>com.gun3y.pagerank.entity.html.HtmlPage</class-b>
		<field>
			<a set-method="setWebURL" get-method="getWebURL">url</a>
			<b>url</b>
		</field>
		<field>
			<a>parseData</a>
			<b>htmlData</b>
			<a-hint>edu.uci.ics.crawler4j.parser.HtmlParseData</a-hint>
			<b-hint>com.gun3y.pagerank.entity.html.HtmlData</b-hint>
		</field>
	</mapping>
	<mapping>
		<class-a>edu.uci.ics.crawler4j.parser.HtmlParseData</class-a>
		<class-b>com.gun3y.pagerank.entity.html.HtmlData</class-b>
	</mapping>
	<mapping>
		<class-a>edu.uci.ics.crawler4j.url.WebURL</class-a>
		<class-b>com.gun3y.pagerank.entity.html.WebUrl</class-b>
		<field>
			<a set-method="setURL" get-method="getURL">url</a>
			<b>url</b>
		</field>
	</mapping>
</mappings>